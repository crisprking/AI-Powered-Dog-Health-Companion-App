{
  "expo": {
    "name": "Luna Rising",
    "slug": "rork-nest-focus",
    "version": "1.0.0",
    "orientation": "portrait",
    "platforms": ["ios"],
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "dark",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#000000"
    },
    "ios": {
      "bundleIdentifier": "com.rork.nest.focus",
      "supportsTablet": false,
      "requireFullScreen": true,
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false,
        "NSUserTrackingUsageDescription": "This app uses tracking to provide personalized AI insights and improve your habit tracking experience."
      }
    },
    "bundleIdentifier": "com.rork.nest.focus",
    "plugins": [
      "expo-router"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "eas": {
        "projectId": "60f3bf10-fdc8-459e-8fc4-af34894231c6"
      }
    },
    "runtimeVersion": {
      "policy": "appVersion"
    }
  }
}