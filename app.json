{
  "expo": {
    "name": "Soluna: Habit Transformation",
    "slug": "soluna-habit-transformation",
    "version": "1.0.0",
    "orientation": "portrait",
    "platforms": ["ios"],
    "icon": "./rork-nest-focus/rork-soluna--habit-transformation/assets/images/icon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "splash": {
      "image": "./rork-nest-focus/rork-soluna--habit-transformation/assets/images/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": false,
      "bundleIdentifier": "app.rork.soluna-habit-transformation",
      "requireFullScreen": true,
      "buildNumber": "1",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false,
        "UIDeviceFamily": [1],
        "CFBundleAllowMixedLocalizations": true,
        "CFBundleLocalizations": [
          "en", "fr"
        ],
        "NSPhotoLibraryUsageDescription": "Allow $(PRODUCT_NAME) to access your photos to share your habit progress",
        "NSCameraUsageDescription": "Allow $(PRODUCT_NAME) to access your camera to take photos of your habits",
        "NSMicrophoneUsageDescription": "Allow $(PRODUCT_NAME) to access your microphone for voice notes about your habits"
      }
    },
    "plugins": [
      [
        "expo-router",
        {
          "origin": "https://rork.com/"
        }
      ],
      [
        "expo-image-picker",
        {
          "photosPermission": "The app accesses your photos to let you share them with your friends."
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "eas": {
        "projectId": "3dbaee6b-a8d2-41af-aff9-0317755a4056"
      }
    }
  }
}